{
    "name": "baptouuuu/series",
    "type": "project",
    "description": "Series watcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "innmind/cli": "^1.2",
        "innmind/immutable": "^2.8",
        "innmind/html": "~4.0",
        "innmind/crawler": "~5.0",
        "innmind/time-continuum": "^1.1",
        "innmind/server-control": "^2.7"
